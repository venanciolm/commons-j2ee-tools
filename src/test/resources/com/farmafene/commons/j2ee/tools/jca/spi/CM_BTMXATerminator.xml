<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.w3.org/2001/XMLSchema-instance
        http://www.w3.org/2001/XMLSchema.xsd
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean id="connectionFactory1"
		class="com.farmafene.commons.j2ee.tools.jca.btm.BTMConectionFactoryFactoryBean">
		<property name="name" value="managedConnectionFactory1" />
		<property name="minPoolSize" value="2" />
		<property name="maxPoolSize" value="2" />
		<property name="managedConnectionFactory" ref="managedConnectionFactory1" />
	</bean>
	<bean id="connectionFactory2"
		class="com.farmafene.commons.j2ee.tools.jca.btm.BTMConectionFactoryFactoryBean">
		<property name="name" value="managedConnectionFactory2" />
		<property name="minPoolSize" value="2" />
		<property name="maxPoolSize" value="2" />
		<property name="managedConnectionFactory" ref="managedConnectionFactory2" />
	</bean>
	<bean id="connectionFactory3"
		class="com.farmafene.commons.j2ee.tools.jca.btm.BTMConectionFactoryFactoryBean">
		<property name="name" value="datasource1" />
		<property name="minPoolSize" value="2" />
		<property name="maxPoolSize" value="2" />
		<property name="managedConnectionFactory" ref="managedConnectionFactory3" />
	</bean>
</beans>